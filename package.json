{
	"name": "fingerprint-injector",
	"version": "1.0.0",
	"description": "Browser fingerprint injection library for playwright and puppeteer.",
	"engines": {
		"node": ">=15.10.0"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"dependencies": {
		"@apify/log": "^1.0.5",
		"tslib": "^2.3.1",
		"useragent": "^2.3.0"
	},
	"devDependencies": {
		"@apify/eslint-config-ts": "^0.1.4",
		"@apify/tsconfig": "^0.1.0",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.10.1",
		"@types/puppeteer": "^5.4.4",
		"@types/useragent": "^2.3.1",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"eslint": "^7.0.0",
		"fingerprint-generator": "^1.0.0-beta.1",
		"fs-extra": "^10.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"jest": "^27.2.5",
		"jest-circus": "^27.2.4",
		"jsdoc-to-markdown": "^7.0.0",
		"markdown-toc": "^1.2.0",
		"playwright": "^1.12.2",
		"puppeteer": "^10.4.0",
		"ts-jest": "^27.0.5",
		"ts-node": "^10.2.1",
		"typescript": "^4.4.3"
	},
	"scripts": {
		"build": "rimraf dist && tsc",
		"postbuild": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"prepublishOnly": "npm run build",
		"lint": "eslint src test",
		"lint:fix": "eslint src test --fix",
		"test": "npm run build && jest",
		"build-docs": "npm run build && npm run build-toc && node docs/build-docs.js",
		"build-toc": "markdown-toc docs/README.md -i"
	},
	"author": {
		"name": "Apify",
		"email": "support@apify.com",
		"url": "https://apify.com"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/apify/fingerprint-injector"
	}
}
